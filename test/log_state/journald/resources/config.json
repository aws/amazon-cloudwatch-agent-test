{
  "agent": {
    "debug": true
  },
  "logs": {
    "logs_collected": {
      "journald": {
        "collect_list": [
          {
            "log_group_name": "journald-test-{instance_id}",
            "log_stream_name": "{instance_id}",
            "retention_in_days": 7,
            "units": ["systemd", "sshd"],
            "filters": [
              {
                "type": "include",
                "expression": "Started"
              },
              {
                "type": "exclude",
                "expression": "debug"
              }
            ]
          }
        ]
      }
    }
  }
}