{
  "agent": {
    "metrics_collection_interval": 10,
    "run_as_user": "root",
    "debug": true,
    "logfile": ""
  },
  "logs": {
    "metrics_collected": {
      "prometheus": {
        "prometheus_config_path": "/tmp/prometheus.yaml",
        "log_group_name": "{JobName}",
        "emf_processor": {
          "metric_namespace": "${NAMESPACE}",
          "metric_declaration": [
            {
              "source_labels": [
                "include"
              ],
              "label_matcher": "^yes$",
              "dimensions": [
                [
                  "prom_type"
                ],
                [
                  "prom_type",
                  "quantile"
                ]
              ],
              "metric_selectors": [
                "^prometheus_test*"
              ]
            }
          ]
        }
      }
    },
    "force_flush_interval": 5
  }
}