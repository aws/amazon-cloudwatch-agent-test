{
  "agent": {
    "metrics_collection_interval": 15,
    "run_as_user": "root",
    "debug": true,
    "aws_sdk_log_level": "LogDebugWithHTTPBody",
    "logfile": ""
  },
  "logs": {
    "metrics_collected": {
      "kubernetes": {
        "enhanced_container_insights": true
      }
    },
    "force_flush_interval": 5
  }
}