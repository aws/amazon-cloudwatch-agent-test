{
    "agent": {
        "metrics_collection_interval": 60,
        "run_as_user": "root",
        "debug": true,
        "logfile": ""
    },
    "metrics": {
        "namespace": "MetricValueBenchmarkTest",
        "append_dimensions": {
            "InstanceId": "${aws:InstanceId}"
        },
        "metrics_collected": {
            "swap": {
                "measurement": [
                    "free",
                    "used",
                    "used_percent"
                ]
            }
        }
    }
}