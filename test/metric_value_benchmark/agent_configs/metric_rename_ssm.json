{
  "agent": {
    "metrics_collection_interval": 10,
    "debug": true,
    "credentials": {
      "role_arn": "arn:aws:iam::ACCOUNT_ID:role/ROLE_NAME"
    }
  },
  "metrics": {
    "namespace": "IPv6AllClientsTest",
    "metrics_collected": {
      "cpu": {
        "resources": [
          "*"
        ],
        "measurement": [
          {"name": "cpu_usage_nice", "rename": "ssm_cpu_utilization", "unit": "Percent"}
        ]
      },
      "mem": {
        "measurement": [
          "used_percent"
        ],
        "metrics_collection_interval": 10
      }
    }
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/messages",
            "log_group_name": "IPv6AllClientsTest/Logs",
            "log_stream_name": "{instance_id}",
            "timezone": "UTC"
          }
        ]
      }
    }
  },
  "traces": {
    "traces_collected": {
      "xray": {}
    }
  }
}
