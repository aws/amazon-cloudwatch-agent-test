{
    "agent": {
        "metrics_collection_interval": 15,
        "run_as_user": "root",
        "debug": true,
        "logfile": ""
      },
      "metrics": {
        "namespace": "MetricValueBenchmarkTest",
        "append_dimensions": {
          "InstanceId": "${aws:InstanceId}"
        },
        "metrics_collected": {
            "ethtool": {
                "interface_include": [
                    "docker0"
                ],
                "metrics_include": [
                        "rx_packets",
                        "tx_packets",
                        "rx_bytes",
                        "tx_bytes"
                      ]
            }
        },
        "force_flush_interval": 5
      }
}