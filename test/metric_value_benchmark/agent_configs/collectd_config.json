{
    "metrics": {
      "namespace": "MetricValueBenchmarkTest",
      "append_dimensions": {
        "InstanceId": "${aws:InstanceId}"
      },
      "metrics_collected": {
        "collectd": {
          "metrics_aggregation_interval": 30,
          "collectd_security_level": "none"
        }
      },
      "force_flush_interval": 5
    }
}