{
  "agent": {
    "metrics_collection_interval": 60,
    "run_as_user": "root",
    "debug": true,
    "logfile": ""
  },
  "metrics": {
    "namespace": "MetricValueBenchmarkTest",
    "append_dimensions": {
      "InstanceId": "${aws:InstanceId}"
    },
    "metrics_collected": {
      "diskio": {
        "resources": [
          "*"
        ],
        "measurement": [
          "iops_in_progress", "io_time", "reads", "read_bytes", "read_time", "writes", "write_bytes", "write_time"
        ]
      }
    }
  }
}