  "type": "object",
  "properties": {
    "CloudWatchMetrics": {},
    "ClusterName": {},
    "Namespace": {},
    "NodeName": {},
    "PersistentVolumeClaimName": {},
    "PlatformType": {},
    "Sources": {},
