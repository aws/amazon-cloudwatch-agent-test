{
  "agent": {
    "metrics_collection_interval": 60,
    "run_as_user": "root",
    "debug": true,
    "logfile": ""
  },
  "metrics": {
    "namespace": "MetricAggregateDimensionTest",
    "append_dimensions": {
      "InstanceId": "${aws:InstanceId}",
      "InstanceType": "${aws:InstanceType}",
    },
    "aggregate_dimensions": [["InstanceType"], []],
    "metrics_collected": {
      "cpu": {
        "measurement": [
          "time_active", "time_guest"
        ]
      }
    }
  }
}