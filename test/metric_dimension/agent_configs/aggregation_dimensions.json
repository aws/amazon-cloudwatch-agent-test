{
    "agent": {
        "metrics_collection_interval": 10
    },
    "metrics": {
        "force_flush_interval": 5,
        "namespace": "TestAggregationDimensions",
        "append_dimensions": {
            "InstanceId": "${aws:InstanceId}",
            "InstanceType": "${aws:InstanceType}"
        },
        "aggregation_dimensions": [
            [],
            [
                "InstanceId"
            ],
            [
                "InstanceId",
                "InstanceType"
            ],
            [
                "foo",
                "bar",
                "InstanceType"
            ]
        ],
        "metrics_collected": {
            "mem": {
                "append_dimensions": {
                    "foo": "fooval",
                    "bar": "barval",
                    "baz": "bazval"
                },
                "measurement": [
                    "mem_used_percent"
                ]
            },
            "cpu": {
                "measurement": [
                    "usage_user"
                ],
                "metrics_collection_interval": 10
            }
        }
    }
}