{
    "agent": {
        "metrics_collection_interval": 60
    },
    "metrics": {
        "append_dimensions": {
            "InstanceType": "${aws:InstanceType}"
        },
        "metrics_collected": {
            "LogicalDisk": {
                "resources": [
                    "*"
                ],
                "measurement": [
                    "% Free Space",
                    "% Disk Time"
                ]
            },
            "Memory": {
                "measurement": [
                    "% Committed Bytes In Use"
                ]
            },
            "Processor": {
                "resources": [
                    "*"
                ],
                "measurement": [
                    "% User Time",
                    "% Idle Time",
                    "% Interrupt Time",
                    "% Processor Time"
                ]
            },
            "statsd": {},
            "System": {
                "measurement": [
                    "Context Switches/sec"
                ]
            },
            "procstat": [
                {
                    "measurement": [
                        "cpu_usage",
                        "memory_rss",
                        "num_threads"
                    ],
                    "exe": "amazon-cloudwatch-agent"
                }
            ]
        },
        "namespace": "CWAgent/SoakTestLowWindows"
    },
    "logs": {
        "endpoint_override": "http://localhost:4566",
        "logs_collected": {
            "files": {
                "collect_list": [
                    {
                        "file_path": "C:\\tmp\\soakTest\\tmp0*.log",
                        "log_group_name": "WS0.log",
                        "multi_line_start_pattern": "{timestamp_format}",
                        "timestamp_format": "%d %b %y %H:%M:%S",
                        "timezone": "UTC",
                        "retention_in_days": 1
                    },
                    {
