{
    "agent": {
        "metrics_collection_interval": 10
    },
    "metrics": {
        "endpoint_override": "http://localhost:4566",
        "append_dimensions": {
            "InstanceType": "${aws:InstanceType}"
        },
        "metrics_collected": {
            "cpu": {
                "measurement": [
                    "cpu_usage_idle",
                    "cpu_usage_iowait",
                    "cpu_usage_user",
                    "cpu_usage_system"
                ],
                "totalcpu": false
            },
            "disk": {
                "measurement": [
                    "used_percent",
                    "inodes_free"
                ]
            },
            "diskio": {
                "measurement": [
                    "io_time"
                ]
            },
            "mem": {
                "measurement": [
                    "mem_used_percent"
                ]
            },
            "statsd": {},
            "swap": {
                "measurement": [
                    "swap_used_percent"
                ]
            },
            "procstat": [
                {
                    "measurement": [
                        "cpu_usage",
                        "memory_rss"
                    ],
                    "exe": "amazon-cloudwatch-agent"
                }
            ]
        },
        "namespace": "CWAgent/LinuxSoakTest"
    },
    "logs": {
        "endpoint_override": "http://localhost:4566",
        "logs_collected": {
            "files": {
                "collect_list": [
                    {
                        "file_path": "/tmp/soakTest/tmp0*.log",
                        "log_group_name": "LinuxSoaking0.log",
                        "multi_line_start_pattern": "{timestamp_format}",
                        "timestamp_format": "%d %b %y %H:%M:%S",
                        "timezone": "UTC",
                        "retention_in_days": 1
                    },
                    {
                        "file_path": "/tmp/soakTest/tmp1*.log",
                        "log_group_name": "LinuxSoaking1.log",
