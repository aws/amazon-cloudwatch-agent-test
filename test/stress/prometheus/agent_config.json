{
  "agent": {
    "debug": true
  },
  "logs": {
    "metrics_collected": {
      "prometheus": {
        "prometheus_config_path": "/tmp/prometheus.yaml",
        "log_group_name": "CloudWatchAgentStress/Prometheus",
        "emf_processor": {
          "metric_namespace": "CloudWatchAgentStress/Prometheus",
          "metric_declaration": [
            {
              "source_labels": ["job"],
              "label_matcher": ".*",
              "dimensions": [
                ["job"],
                ["job", "instance"],
                ["job", "prom_metric_type"],
                ["job", "series_id"]
              ],
              "metric_selectors": [
                "^avalanche_.*"
              ]
            }
          ]
        }
      }
    },
    "force_flush_interval": 5
  },
  "metrics": {
    "namespace": "CloudWatchAgentStress",
    "metrics_collected": {
      "procstat": [
        {
          "exe": "cloudwatch-agent",
          "measurement": [
            "cpu_usage",
            "memory_rss",
            "memory_swap",
            "memory_vms",
            "memory_data",
            "num_fds"
          ],
          "metrics_collection_interval": 1
        }
      ]
    },
    "append_dimensions": {
      "InstanceId": "${aws:InstanceId}"
    }
  }
}