{
  "agent": {
    "debug": true
  },
  "metrics": {
    "namespace": "MetricGlobalAppendDimension",
    "append_dimensions":
    {
        "AutoScalingGroupName": "${aws:AutoScalingGroupName}",
        "ImageId": "${aws:ImageId}",
        "InstanceId": "${aws:InstanceId}",
        "InstanceType": "${aws:InstanceType}"
    },
    "metrics_collected": {
      "cpu": {
        "measurement": ["time_active"]
      }
    },
    "force_flush_interval": 5
  }
}