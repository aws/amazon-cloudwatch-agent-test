{
  "resourceMetrics": [
    {
      "resource": {
        "attributes": [
          {
            "key": "service.name",
            "value": {
              "stringValue": "my.service"
            }
          }
        ]
      },
      "scopeMetrics": [
        {
          "scope": {
            "name": "my.library",
            "version": "1.0.0",
            "attributes": [
              {
                "key": "my.scope.attribute",
                "value": {
                  "stringValue": "some scope attribute"
                }
              }
            ]
          },
          "metrics": [
            {
              "name": "my.delta.histogram",
              "unit": "1",
              "description": "I am a Delta Histogram",
              "histogram": {
                "aggregationTemporality": 1,
                "dataPoints": [
                  {
                    "startTimeUnixNano": METRIC_TIME,
                    "timeUnixNano": METRIC_TIME,
                    "count": 2,
                    "sum": 2,
                    "bucketCounts": [0,2],
                    "explicitBounds": [1,2],
                    "min": 0,
                    "max": 2,
                    "attributes": [
                      {
                        "key": "my.delta.histogram.attr",
                        "value": {
                          "stringValue": "some value"
                        }
                      },
                      {
                        "key": "instance_id",
                        "value": {
                          "stringValue": "$INSTANCE_ID"
                        }
                      }
                    ]
                  }
                ]
              }
            },
            {
              "name": "my.cumulative.histogram",
              "unit": "1",
              "description": "I am a Cumulative Histogram",
              "histogram": {
                "aggregationTemporality": 2,
                "dataPoints": [
                  {
                    "startTimeUnixNano": INITIAL_METRIC_TIME,
                    "timeUnixNano": METRIC_TIME,
                    "count": CUMULATIVE_HIST_COUNT,
                    "sum": CUMULATIVE_HIST_SUM,
                    "bucketCounts": [0,CUMULATIVE_HIST_COUNT],
                    "explicitBounds": [1, 2],
                    "min": 0,
                    "max": 2,
                    "attributes": [
                      {
                        "key": "my.cumulative.histogram.attr",
                        "value": {
                          "stringValue": "some value"
                        }
                      },
                      {
                        "key": "instance_id",
                        "value": {
                          "stringValue": "$INSTANCE_ID"
                        }
                      }
                    ]
                  }
                ]
              }
            },
            {
              "name": "my.delta.exponential.histogram",
              "unit": "1",
              "description": "I am an Delta Exponential Histogram",
              "exponentialHistogram": {
                "aggregationTemporality": 1,
                "dataPoints": [
                  {
                    "startTimeUnixNano": INITIAL_METRIC_TIME,
                    "timeUnixNano": METRIC_TIME,
                    "count": 3,
                    "sum": 10,
                    "scale": 0,
                    "zeroCount": 1,
                    "positive": {
                      "offset": 1,
                      "bucketCounts": [0,2]
                    },
                    "min": 0,
                    "max": 5,
                    "zeroThreshold": 0,
                    "attributes": [
                      {
                        "key": "my.delta.exponential.histogram.attr",
                        "value": {
                          "stringValue": "some value"
                        }
                      },
                      {
                        "key": "instance_id",
                        "value": {
                          "stringValue": "$INSTANCE_ID"
                        }
                      }
                    ]
                  }
                ]
              }
            },
            {
              "name": "my.cumulative.exponential.histogram",
              "unit": "1",
              "description": "I am a Cumulative Exponential Histogram",
              "exponentialHistogram": {
                "aggregationTemporality": 2,
                "dataPoints": [
                  {
                    "startTimeUnixNano": INITIAL_METRIC_TIME,
                    "timeUnixNano": METRIC_TIME,
                    "count": CUMULATIVE_HIST_COUNT,
                    "sum": CUMULATIVE_HIST_SUM,
                    "scale": 0,
                    "zeroCount": 1,
                    "positive": {
                      "offset": 1,
                      "bucketCounts": [0,CUMULATIVE_HIST_COUNT]
